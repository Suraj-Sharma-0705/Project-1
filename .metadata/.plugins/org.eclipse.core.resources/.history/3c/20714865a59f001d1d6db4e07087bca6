package com.springcore.lifecycle;

public class Samosa {
	private double price;	
	
	public double getprice() {
		return price;
	}
	
	public void setprice(double price) {
		this.price=price;
	}
	
	public Samosa() {
		super();
	}
	
	@Override
	public String toString() {
		return "Samosa[ price=" + price +"]";
	}
	
}
